{
  "routes": [
    {
      "route": "/pricing",
      "rewrite": "/pricing.html"
    },
    {
      "route": "/app",
      "rewrite": "/app.html"
    },
    {
      "route": "/admin",
      "rewrite": "/admin.html"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; connect-src https: wss: blob:; img-src https: data: blob:; object-src 'none'"
  }
}